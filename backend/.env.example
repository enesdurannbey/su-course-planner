# Backend Environment Variables
# Copy this file to .env and update with your values

# CORS Origins (comma-separated list of allowed origins)
# For development:
CORS_ORIGINS=http://localhost,http://localhost:5173,http://localhost:8000,http://127.0.0.1:5173,http://127.0.0.1:8000

# For production (Vercel/hosted frontend):
# CORS_ORIGINS=https://su-course-planner.vercel.app,https://your-domain.com

# Environment type (development or production)
ENVIRONMENT=development
